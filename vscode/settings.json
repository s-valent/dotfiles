            "editor.wordHighlightBackground": "#5b7e5660",
            "editor.wordHighlightStrongBackground": "#9e5b3660",
            "editorBracketMatch.background": "#ffffff15",
            "editorBracketMatch.border": "#ffffff00",
            "editorStickyScroll.border": "#000000",
            "inlineChat.border": "#000000",
            "panelStickyScroll.border": "#000000",
            "sideBarStickyScroll.border": "#000000",
