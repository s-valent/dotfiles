    "breadcrumbs.enabled": false,
    "chat.commandCenter.enabled": false,
    "chat.experimental.offerSetup": false,
    "clangd.detectExtensionConflicts": false,
    "diffEditor.diffAlgorithm": "advanced",
    "diffEditor.experimental.showMoves": true,
    // "diffEditor.ignoreTrimWhitespace": false,
    "editor.bracketPairColorization.enabled": false,
    // "editor.cursorBlinking": "solid",
    "editor.dragAndDrop": false,
    "editor.emptySelectionClipboard": false,
    "editor.experimental.preferTreeSitter": [
        "typescript"
    ],
    "editor.guides.highlightActiveIndentation": false,
    "editor.guides.indentation": false,
    "editor.hideCursorInOverviewRuler": true,
    "editor.inlayHints.enabled": "off",
    "editor.inlineSuggest.suppressSuggestions": true,
    "editor.largeFileOptimizations": false,
    // "editor.lineNumbers": "relative",
    "editor.minimap.enabled": false,
    "editor.multiCursorModifier": "ctrlCmd",
    // "editor.padding.bottom": 205,
    // "editor.padding.top": 15,
    "editor.renderLineHighlight": "all",
    "editor.scrollBeyondLastColumn": 0,
    "editor.scrollBeyondLastLine": false,
    "editor.scrollbar.horizontal": "hidden",
    "editor.scrollbar.verticalScrollbarSize": 10,
    "editor.selectionHighlight": false,
    "editor.semanticHighlighting.enabled": false,
    "editor.showFoldingControls": "never",
    "editor.smoothScrolling": true,
    "editor.stickyScroll.enabled": false,
    "editor.unicodeHighlight.ambiguousCharacters": false,
    "editor.unicodeHighlight.includeStrings": false,
    "editor.wordWrap": "on",
    "explorer.autoRevealExclude": {
        "**/venv": true,
        "**/.venv": true
    },
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "extensions.experimental.affinity": {
        "asvetliakov.vscode-neovim": 1
    },
    "extensions.ignoreRecommendations": true,
    "files.associations": {
        "*.jinja2": "jinja-py"
    },
    "files.exclude": {
        "**/__pycache__": true,
        "**/.idea": true,
        "**/.ipynb_checkpoints": true,
        "**/*.egg-info": true,
        "**/.ruff_cache": true,
        "**/.pytest_cache": true
    },
    "files.insertFinalNewline": true,
    "files.trimTrailingWhitespace": true,
    "fortran.fortls.disabled": true,
    "git.ignoreLegacyWarning": true,
    "git.openRepositoryInParentFolders": "never",
    "julia.enableTelemetry": false,
    "julia.symbolCacheDownload": true,
    "jupyter.askForKernelRestart": false,
    "material-icon-theme.folders.theme": "classic",
    "material-icon-theme.opacity": 0.7,
    "notebook.cellToolbarLocation": {
        "default": "right",
        "jupyter-notebook": "left"
    },
    "notebook.output.wordWrap": true,
    "python.analysis.autoImportCompletions": true,
    "python.analysis.autoSearchPaths": false,
    "python.analysis.diagnosticMode": "openFilesOnly",
    "python.analysis.extraPaths": [
        ".",
        ".."
    ],
    "python.analysis.indexing": false,
    "python.analysis.typeEvaluation.enableExperimentalFeatures": true,
    "python.autoComplete.extraPaths": [
        ".",
        ".."
    ],
    "python.defaultInterpreterPath": "/usr/local/bin/python",
    "python.terminal.activateEnvironment": false,
    "references.preferredLocation": "view",
    "remote.SSH.experimental.chat": false,
    "remote.SSH.experimental.enhancedSessionLogs": false,
    "remote.autoForwardPorts": false,
    "security.workspace.trust.banner": "never",
    "security.workspace.trust.enabled": false,
    "telemetry.telemetryLevel": "off",
    "terminal.integrated.commandsToSkipShell": [
        "language-julia.interrupt"
    ],
    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.enableMultiLinePasteWarning": "never",
    "terminal.integrated.macOptionIsMeta": true,
    "terminal.integrated.scrollback": 1000000,
    "terminal.integrated.shellIntegration.decorationsEnabled": "never",
    "terminal.integrated.shellIntegration.enabled": false,
    "update.mode": "manual",
    "window.customTitleBarVisibility": "windowed",
    "window.newWindowDimensions": "inherit",
    "window.restoreWindows": "none",
    "window.title": "${activeEditorMedium}",
    "workbench.activityBar.location": "top",
    "workbench.colorTheme": "Tomorrow Gray",
    "workbench.editor.empty.hint": "hidden",
    "workbench.editor.showTabs": "none",
    "workbench.iconTheme": "material-icon-theme",
    "workbench.layoutControl.enabled": false,
    "workbench.localHistory.enabled": false,
    "workbench.panel.defaultLocation": "bottom",
    "workbench.panel.opensMaximized": "preserve",
    "workbench.settings.editor": "json",
    "workbench.startupEditor": "none",

    "[python]": {
        "ruff.enable": false,
        "editor.defaultFormatter": "ms-python.black-formatter"
    },

    "catppuccin.accentColor": "blue",
    "catppuccin.colorOverrides": {
        "frappe": {
            "surface2": "#313445"
        }
    },
    "catppuccin.italicComments": false,
    "catppuccin.italicKeywords": false,

    // "vim.enableNeovim": true,
    // "vim.neovimUseConfigFile": true,
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                "g",
                "r"
            ],
            "commands": [
                "references-view.findReferences"
            ],
            "silent": true
        }
    ],
    // "vim.useSystemClipboard": true,
    // "vim.vimrc.enable": false,
    // "vim.vimrc.path": "~/.vim/vimrc",

    "errorLens.decorations": {
        "errorRange": {
            "textDecoration": ";border-bottom:2px solid #e15a60"
        },
        "warningRange": {
            "textDecoration": ";border-bottom:2px solid #f99157"
        },
        "infoRange": {
            "textDecoration": ";border-bottom:2px solid #00b7e4"
        }
    },
    "errorLens.enableOnDiffView": true,
    "errorLens.enabledDiagnosticLevels": [
        "error",
        "warning",
        "info"
    ],
    "errorLens.enabledInMergeConflict": false,
    "errorLens.messageBackgroundMode": "message",
    "errorLens.messageTemplate": " ■ $message ",
    "errorLens.problemRangeDecorationEnabled": true,
    "errorLens.scrollbarHackEnabled": true,

    "editor.tokenColorCustomizations": {
        "[Vitesse Dark Soft]": {
            "textMateRules": [
                {
                    "scope": "meta.function-call.python, variable.function, support.function, keyword.other.special-method, meta.block-level, meta.function.decorator",
                    "settings": {
                        "foreground": "#B8A965"
                    }
                },
                {
                    "scope": "meta.function-call.arguments, punctuation.definition.arguments",
                    "settings": {
                        "foreground": "#dbd7ca"
                    }
                }
            ]
        }
    },

    "workbench.colorCustomizations": {
        "editorError.foreground": "#fff0",
        "editorWarning.foreground": "#fff0",
        "editorInfo.foreground": "#fff0",
        "[Catppuccin Frappé]": {
