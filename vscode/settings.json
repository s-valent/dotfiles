            "widget.shadow": "#00000000",
            "errorLens.errorForeground": "#cb7676",
            "errorLens.errorForegroundLight": "#cb7676",
            "errorLens.hintForeground": "#4d9375",
            "errorLens.hintForegroundLight": "#4d9375",
            "errorLens.infoForeground": "#5eaab5",
            "errorLens.infoForegroundLight": "#5eaab5",
            "errorLens.statusBarErrorForeground": "#cb7676",
            "errorLens.statusBarHintForeground": "#4d9375",
            "errorLens.statusBarIconErrorForeground": "#cb7676",
            "errorLens.statusBarIconWarningForeground": "#d4976c",
            "errorLens.statusBarInfoForeground": "#5eaab5",
